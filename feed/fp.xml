---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>folone</title>
 <link href="http://www.folone.info/feed/fp.xml" rel="self"/>
 <link href="http://www.folone.info/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.folone.info.com/</id>
 <author>
   <name>folone</name>
   <email>folone@gmail.com.com</email>
 </author>

 {% for post in site.posts %}
 {% for tag in post.categories %}
 {% if tag == "fp" %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.folone.info{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www.folone.info{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endif %}
 {% endfor %}
 {% endfor %}
 
</feed>
